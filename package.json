{
  "name": "multi-pilot",
  "displayName": "Multi-Pilot",
  "publisher": "pubudutharaka",
  "description": "Multi-Pilot is a Visual Studio Code extension that enables developers to interact with multiple AI language models simultaneously, allowing for direct comparison of responses from different AI models",
  "icon": "images/icon.png",
  "version": "1.2.0",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "extensionDependencies": [
    "github.copilot-chat"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "multi-pilot.searchBuAgent",
        "title": "Multi-Pilot Search"
      },
      {
        "command": "multi-pilot.selectModels",
        "title": "Multi-Pilot: Select Models"
      },
      {
        "command": "multi-pilot.enhancePrompt",
        "title": "Multi-Pilot: Enhance Prompt"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "multi-pilot.enhancePrompt",
          "group": "1_modification"
        }
      ],
      "chat/context": [
        {
          "command": "multi-pilot.enhancePrompt",
          "when": "chatLocation == 'panel'",
          "group": "navigation"
        }
      ]
    },
    "chatParticipants": [
      {
        "id": "vscode.multi-pilot",
        "name": "multi-pilot",
        "description": "Multi-pilot chat"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.24.2",
    "eslint": "^9.19.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "marked": "^15.0.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xtharakax/multi-pilot.git"
  },
  "homepage": "https://github.com/xtharakax/multi-pilot/blob/main/README.md"
}
